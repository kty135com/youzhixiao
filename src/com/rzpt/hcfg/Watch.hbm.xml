<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.rzpt.entity.Watch" table="watch">
		<id name="id">
			<generator class="native"></generator>
		</id>
		<property name="name"></property>
		<property name="no"></property>
		<property name="content"></property>
		<property name="plan_time"></property>
		<property name="finish_time"></property>
		<property name="money"></property>
		<property name="realMoney"></property>
		<property name="state"></property>
		<property name="task_id"></property>
		<property name="pro3_id"></property>
		<property name="dept_id"></property>
		<property name="user_id"></property>
		<property name="user1_id"></property>
		<property name="user2_id"></property>
		<property name="user3_id"></property>
		<property name="user4_id"></property>
		<property name="user5_id"></property>
	</class>
</hibernate-mapping>